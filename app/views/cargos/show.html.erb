<%= render partial: '0p/general/icon_title', locals: {c: 'cargos', text: "Cargo : #{@objeto.cargo}"} %>

<%= render partial: '0p/tabs/tabs', locals: { tabs: @tabs, options: @options, token: :tab } %>

<% if @options[:tab] == 'Estructura' %>
	<%= render partial: 'cargos/partials/display_cargo', locals: {cargo: @objeto} %>
<% elsif @options[:tab] == 'Documentos' %>
<div>
  <%= render partial: '0p/general/icon_title', locals: {c: 'control_documentos', text: "Control Documentos Empleados"} %>
  <%= render partial: '0p/tabla/tabla', locals: {c: 'control_documentos'} %>
  <%= render partial: '0p/tabla/btn_nuevo_o', locals: {o: @objeto, c: 'control_documentos'} %>
</div>
<% end %>
